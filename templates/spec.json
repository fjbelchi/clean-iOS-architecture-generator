              {
                "description": "Perform API request to mark a notification as read",
                "return_type": "void",
                "name": "markNotificationAsRead:onCompletionBlock:",
                "arguments": [
                  {
                    "type": "NSString",
                    "name": "notificationsId"
                  },
                  {
                    "type": "^()",
                    "name": "completionBlock"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
